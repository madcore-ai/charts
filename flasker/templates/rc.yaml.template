apiVersion: v1
kind: ReplicationController
metadata:
  labels:
    name: ${APP_NAME}
  name: ${APP_NAME}
  namespace: flasker-plugin
spec:
  replicas: 1
  selector:
    name: ${APP_NAME}
  template:
    metadata:
      labels:
        name: ${APP_NAME}
    spec:
      containers:
      - name: flasker
        image: core.madcore:5000/${APP_NAME}:image
      imagePullSecrets:
      - name: myregistrykey
